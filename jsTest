
var nb=50:
for (var i=0; i<=nb; i++) {
      
 if (i % 2 === 0) {
console.log("fizz"); 
        }
else if(i % 3 === 0){
    console.log("buzz")
}
else(i % 5 === 0) {
    console.log("fizzbuzz")
}
}

var i=100
let (var i=0; i<=100; i++){

    if(i % 3 ===0) {
        console.log("dog")
    }
    else if(i % 5 === 0) {
        console.log("snoop")
    }
    else if(i % 9 ===0) {
        console.log("snoopDog")
    }
}